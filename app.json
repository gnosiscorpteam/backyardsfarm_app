{
  "expo": {
    "name": "Backyards Farm",
    "slug": "BackyardsFarm",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ariannaviola.BackyardsFarm"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "notification": {
      "color": "#ffffff",
      "androidMode": "default",
      "android": {
        "icon": "./assets/icon.png",
        "color": "#ffffff"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff"
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "enableGooglePay": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "46b78e17-a5d1-419e-9678-4e47dfde20f4"
      }
    }
  }
}
